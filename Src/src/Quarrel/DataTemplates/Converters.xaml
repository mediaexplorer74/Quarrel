<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Quarrel.DataTemplates"
    xmlns:baseconvert="using:Quarrel.Converters.Base"
    xmlns:discordconvert="using:Quarrel.Converters.Discord"
    xmlns:convertguild="using:Quarrel.Converters.Guild"
    xmlns:msgconvert="using:Quarrel.Converters.Messages"
    xmlns:profileconvert="using:Quarrel.Converters.Profile">

    <baseconvert:NotNullToBoolConverter x:Key="NotNullToBoolConverter"/>
    <baseconvert:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter"/>

    <discordconvert:PresenseToBrushConverter x:Key="PresenseToBrushConverter"/>

    <convertguild:MutedToOpacityConverter x:Key="MutedToOpacityConverter"/>

    <msgconvert:SizeToHumanizedSizeConverter x:Key="SizeToHumanizedSizeConverter"/>
    <msgconvert:EmbedFieldsConverter x:Key="EmbedFieldsConverter"/>
</ResourceDictionary>
