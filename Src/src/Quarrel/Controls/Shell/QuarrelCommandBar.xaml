                                            <TranslateTransform x:Name="ClipGeometryTransform" Y="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=TemplateSettings.CompactVerticalDelta}"/>
                                        </RectangleGeometry.Transform>
                                    </RectangleGeometry>
                                </Grid.Clip>

                                <Grid x:Name="ContentRoot" VerticalAlignment="Top" Margin="{TemplateBinding Padding}" Height="{TemplateBinding Height}" Background="{TemplateBinding Background}" Opacity="{TemplateBinding Opacity}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid.RenderTransform>
                                        <TranslateTransform x:Name="ContentTransform"/>
                                    </Grid.RenderTransform>

                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- Use a ContentControl rather than a ContentPresenter so that IsEnabled can be set to false
                                    in the Minimal/HiddenClosed states to remove it from being a tab-stop candidate. -->
                                        <ContentControl x:Name="ContentControl" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" ContentTransitions="{TemplateBinding ContentTransitions}" Foreground="{TemplateBinding Foreground}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" IsTabStop="False"/>

                                        <ItemsControl
                                                x:Name="PrimaryItemsControl" HorizontalAlignment="Right" MinHeight="{ThemeResource AppBarThemeMinHeight}" IsTabStop="False" Grid.Column="1">
                                            <ItemsControl.ItemsPanel>
                                                <ItemsPanelTemplate>
                                                    <StackPanel Orientation="Horizontal" />
                                                </ItemsPanelTemplate>
                                            </ItemsControl.ItemsPanel>
                                        </ItemsControl>
                                    </Grid>

                                    <Button x:Name="MoreButton" Foreground="{TemplateBinding Foreground}" Background="Transparent" Style="{StaticResource EllipsisButton}" Padding="16,23,16,0" MinHeight="{TemplateBinding MinHeight}" VerticalAlignment="Top" Grid.Column="1">
                                        <FontIcon x:Name="EllipsisIcon" VerticalAlignment="Center" FontFamily="{ThemeResource SymbolThemeFontFamily}" FontSize="16" Glyph="&#xE10C;" Height="{ThemeResource AppBarExpandButtonCircleDiameter}"/>
                                    </Button>

                                    <Popup x:Name="OverflowPopup">
                                        <Popup.RenderTransform>
                                            <TranslateTransform x:Name="OverflowPopupOffsetTransform"/>
                                        </Popup.RenderTransform>

                                        <Grid x:Name="OverflowContentRoot" MinWidth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CommandBarTemplateSettings.OverflowContentMinWidth}" MaxWidth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CommandBarTemplateSettings.OverflowContentMaxWidth}" MaxHeight="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CommandBarTemplateSettings.OverflowContentMaxHeight}">
                                            <Grid.Clip>
                                                <RectangleGeometry x:Name="OverflowContentRootClip"/>
                                            </Grid.Clip>

                                            <Grid.RenderTransform>
                                                <TranslateTransform x:Name="OverflowContentRootTransform" X="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CommandBarTemplateSettings.OverflowContentHorizontalOffset}"/>
                                            </Grid.RenderTransform>

                                            <CommandBarOverflowPresenter x:Name="SecondaryItemsControl" Style="{TemplateBinding CommandBarOverflowPresenterStyle}" Background="{TemplateBinding Background}" IsEnabled="False" IsTabStop="False">
                                                <CommandBarOverflowPresenter.RenderTransform>
                                                    <TranslateTransform x:Name="OverflowContentTransform"/>
                                                </CommandBarOverflowPresenter.RenderTransform>
                                                <CommandBarOverflowPresenter.ItemContainerStyle>
                                                    <Style TargetType="FrameworkElement">
                                                        <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                                        <Setter Property="Width" Value="NaN"/>
                                                    </Style>
                                                </CommandBarOverflowPresenter.ItemContainerStyle>
                                            </CommandBarOverflowPresenter>
                                        </Grid>
                                    </Popup>
                                    <Rectangle x:Name="HighContrastBorder" x:DeferLoadStrategy="Lazy" Grid.ColumnSpan="2"  Visibility="Collapsed" VerticalAlignment="Stretch" Stroke="{ThemeResource SystemControlForegroundTransparentBrush}" StrokeThickness="1"/>
                                </Grid>
                            </Grid>

                            <Rectangle Opacity="{StaticResource ShadowOpacity}" Height="12" VerticalAlignment="Bottom" IsHitTestVisible="False" Margin="0,0,0,12">
                                <Rectangle.RenderTransform>
                                    <TranslateTransform x:Name="ShadowTransform"/>
                                </Rectangle.RenderTransform>
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                        <GradientStop Offset="0" Color="{ThemeResource ShadowColor}"/>
                                        <GradientStop Offset="1" Color="#00000000"/>
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </CommandBar.Style>
    
    <CommandBar.Content>
        <Grid>
            <StackPanel Orientation="Horizontal" Height="40">
                <!--Hamburger button-->
                <AppBarButton Height="40" Visibility="{x:Bind baseconvert:NotBoolToVisibilityConverter.Convert(ShowHamburger)}" Click="InvokeHumburgerClick" Canvas.ZIndex="1" Background="Transparent" Width="72">
                    <AppBarButton.Content>
                        <FontIcon Glyph="&#xE700;" FontSize="24" Margin="0,4,0,0"/>
                    </AppBarButton.Content>
                </AppBarButton>
                
                <!--Current Channel info-->
                <StackPanel Height="40" Visibility="{x:Bind baseconvert:NotNullToVisibilityConverter.Convert(ViewModel.CurrentChannel), Mode=OneWay, FallbackValue=Collapsed}"
                            Orientation="Horizontal" VerticalAlignment="Center" Margin="12,0,0,0" HorizontalAlignment="Left" Tapped="ChannelNameTapped">
                    <!--Guild Channel-->
                    <TextBlock VerticalAlignment="Center" Text="#" Visibility="{x:Bind ViewModel.CurrentChannel.IsTextChannel, Mode=OneWay}" Opacity="{x:Bind ViewModel.CurrentChannel.TextOpacity, Mode=OneWay}" Foreground="{StaticResource Foreground}" FontSize="20" FontWeight="Light" Margin="0,0,4,0"/>
                    <!--DM Channel-->
                    <TextBlock VerticalAlignment="Center" Text="@" Visibility="{x:Bind ViewModel.CurrentChannel.IsDirectChannel, Mode=OneWay}" Opacity="{x:Bind ViewModel.CurrentChannel.TextOpacity, Mode=OneWay}" Foreground="{StaticResource Foreground}" FontSize="20" FontWeight="Light" Margin="0,0,4,0"/>
                    <!--Group Channel-->
                    <TextBlock VerticalAlignment="Center" Text="&#xE716;" FontFamily="Segoe MDL2 Assets" Visibility="{x:Bind ViewModel.CurrentChannel.IsGroupChannel, Mode=OneWay}" Opacity="{x:Bind ViewModel.CurrentChannel.TextOpacity, Mode=OneWay}" Foreground="{StaticResource Foreground}" FontSize="20" FontWeight="Light" Margin="0,0,4,0"/>
                    <!--Channel Name-->
                    <TextBlock VerticalAlignment="Center" Text="{x:Bind ViewModel.CurrentChannel.FormattedName, Mode=OneWay}" FontWeight="SemiBold" FontSize="18"/>
                    <!--Seperator-->
                    <AppBarSeparator/>
                    <!--Channel Description-->
                    <controls:MarkdownTextBlock VerticalAlignment="Center" Text="{x:Bind baseconvert:MultipleWhiteSpaceCollapseConverter.Convert(ViewModel.CurrentChannel.AsGuildChannel.Topic), FallbackValue={x:Null}, Mode=OneWay}" FontSize="18" FontWeight="Light" Margin="0,0,4,0" IsTextSelectionEnabled="False"
                                                Visibility="{x:Bind baseconvert:NotBoolToVisibilityConverter.Convert(ViewModel.CurrentGuild.IsDM), Mode=OneWay}"/>

                </StackPanel>
            </StackPanel>
        </Grid>
    </CommandBar.Content>
    
    <CommandBar.PrimaryCommands>
        <!--Call button-->
        <AppBarButton Background="Transparent" Label="Call" Command="{x:Bind ViewModel.CurrentChannel.StartCallCommand, Mode=OneWay}"
                      Visibility="{x:Bind ViewModel.CurrentChannel.Guild.IsDM, FallbackValue=Collapsed, Mode=OneWay}">
            <AppBarButton.Content>
                <FontIcon Glyph="&#xE13A;" FontSize="24" Margin="0,4,0,0"/>
            </AppBarButton.Content>
        </AppBarButton>
        
        <!--Toggle MemberList button-->
        <AppBarButton Background="Transparent" Label="Toggle Member List" Click="InvokeMemberListToggleClick">
            <AppBarButton.RenderTransform>
                <ScaleTransform x:Name="MemberToggleTransform" ScaleX="1" ScaleY="1"/>
            </AppBarButton.RenderTransform>
            <AppBarButton.Content>
                <FontIcon Glyph="&#xE125;" FontSize="24" Margin="0,4,0,0"/>
            </AppBarButton.Content>
        </AppBarButton>
    </CommandBar.PrimaryCommands>

    <CommandBar.SecondaryCommands>
        <AppBarButton x:Uid="/Controls/SettingsABB" Label="Settings" Background="Transparent" Command="{x:Bind ViewModel.OpenSettings}"/>
        <AppBarButton x:Uid="/Controls/CreditABB" Label="Credit" Background="Transparent" Command="{x:Bind ViewModel.OpenCredit}"/>
        <AppBarButton x:Uid="/Controls/WhatsNewABB" Label="What's New" Background="Transparent" Command="{x:Bind ViewModel.OpenWhatsNew}"/>
        <AppBarButton x:Uid="Controls/AboutABB" Label="About" Background="Transparent" Command="{x:Bind ViewModel.OpenAbout}"/>
    </CommandBar.SecondaryCommands>
</CommandBar>
