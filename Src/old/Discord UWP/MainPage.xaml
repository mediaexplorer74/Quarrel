            </ObjectAnimationUsingKeyFrames>-->
            <DoubleAnimation To="1" Duration="0:0:0.5" Storyboard.TargetName="MemberToggle" Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"/>
        </Storyboard>
        <Storyboard x:Name="HideMemberToggle" Duration="0:0:0.5">
