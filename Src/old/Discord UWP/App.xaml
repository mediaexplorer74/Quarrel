                    <SolidColorBrush x:Key="LinkColor" Color="#0096cf"/>
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="Background" Color="#1B1B1B"/>
                    <Color x:Key="AcrylicMessageBackgroundColorTransparent">#00303238</Color>
                    <Color x:Key="AcrylicMessageBackgroundColor">#FF303238</Color>
                    <SolidColorBrush x:Key="AcrylicMessageBackground" Color="#FF303238"/>
                    <!--<AcrylicBrush x:Key="AcrylicMessageBackground"
                    BackgroundSource="HostBackdrop"
                    TintColor="#FF33353b"
                    TintOpacity="1"
                    FallbackColor="#FF222529"/>-->
                    <SolidColorBrush x:Key="AcrylicChannelPaneBackground" Color="#FF1C1D20"/>
                    <!--<AcrylicBrush x:Key="AcrylicChannelPaneBackground"
                    BackgroundSource="HostBackdrop"
                    TintColor="#FF2a2c31"
                    TintOpacity="0.8"
                    FallbackColor="#FF2F3136"/>-->
                    <SolidColorBrush x:Key="AcrylicGuildPaneBackground" Color="#FF141618"/>
                    <!--<AcrylicBrush x:Key="AcrylicGuildPaneBackground"
                    BackgroundSource="HostBackdrop"
                    TintColor="#FF1E2124"
                    TintOpacity="0.6"
                    FallbackColor="#FF1E2124"/>-->
                    <SolidColorBrush x:Key="AcrylicCommandBarBackground" Color="#FF141618"/>
                    <!--<AcrylicBrush x:Key="AcrylicCommandBarBackground"
                    BackgroundSource="HostBackdrop"
                    TintColor="#FF1E2124"
                    TintOpacity="0.7"
                    FallbackColor="#FF1E2124"/>-->
                    <SolidColorBrush x:Key="AcrylicFlyoutBackground" Color="#DD000000"/>
                    <!--<AcrylicBrush x:Key="AcrylicFlyoutBackground"
                    BackgroundSource="HostBackdrop"
                    TintColor="#DD000000"
                    TintOpacity="0.8"
                    FallbackColor="#AA000000"/>-->
                    <SolidColorBrush x:Key="AcrylicUserBackground" Color="#FF303238"/>
                    <SolidColorBrush x:Key="AcrylicUserBackgroundDark" Color="Black"/>
                    <SolidColorBrush x:Key="AcrylicUserBackgroundDarker" Color="#FF141618"/>
                    <SolidColorBrush x:Key="Foreground" Color="White"/>
                    <Color x:Key="ForegroundColor">White</Color>
                    <SolidColorBrush x:Key="Greyple" Color="#99AAB5"/>
                    <SolidColorBrush x:Key="LiteBG" Color="#2a2c31"/>
                    <SolidColorBrush x:Key="DeepBG" Color="#222529"/>

                    <SolidColorBrush x:Key="DarkBG" Color="#1e2124"/>
                    <Color x:Key="DarkBGColor">#1e2124</Color>
                    <Color x:Key="DarkBGColorTransparent">#00222326</Color>
                    <SolidColorBrush x:Key="MidBG" Color="#2a2c31"/>
                    <SolidColorBrush x:Key="MidBG_hover" Color="#2e3036"/>
                    <SolidColorBrush x:Key="MidBG_focus" Color="#303238"/>
                    <SolidColorBrush x:Key="LightBG" Color="#2e3136"/>
                    <SolidColorBrush x:Key="InvertedBG" Color="#e2e7f7"/>
                    <SolidColorBrush x:Key="MessageForeground" Color="#BBFFFFFF"/>
