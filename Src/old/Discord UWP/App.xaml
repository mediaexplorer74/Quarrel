                    FallbackColor="#DDFFFFFF"/>-->
                    <SolidColorBrush x:Key="AcrylicUserBackground" Color="#FFF2F2F3"/>
                    <SolidColorBrush x:Key="AcrylicUserBackgroundDark" Color="#FFF2F2F3"/>
                    <SolidColorBrush x:Key="AcrylicUserBackgroundDarker" Color="#FFE0E0E5"/>
                    <SolidColorBrush x:Key="Foreground" Color="Black"/>
                    <Color x:Key="ForegroundColor">Black</Color>
                    <SolidColorBrush x:Key="Greyple" Color="#99AAB5"/>
                    <SolidColorBrush x:Key="LiteBG" Color="#e0ebfa"/>
                    <SolidColorBrush x:Key="DeepBG" Color="#ffff"/>

